{
  "name": "vladislav-ushmankin-portfolio",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start:client:dev": "ng serve --configuration=development",
    "start:client:prod": "ng serve --configuration=production",
    "build": "ng build",
    "watch": "ng build --watch --configurationon development",
    "test": "ng test",
    "lint": "ng lint",
    "serve:ssr": "node dist/me/server/main.js",
    "start:ssr:prod": "ng run me:serve-ssr:production",
    "start:ssr:dev": "ng run me:serve-ssr:development",
    "build:ssr:prod": "ng build --configuration=production && ng run me:server:production",
    "build:ssr:dev": "ng build --configuration=development && ng run me:server:development",
    "prerender:prod": "ng run me:prerender:production",
    "prerender:dev": "ng run me:prerender:development",
    "sitemap:generate:prod": "node scripts/run.sitemap-generator.js prod",
    "sitemap:generate:dev": "node scripts/run.sitemap-generator.js dev",
    "robots-txt:generate:prod": "node scripts/run.robots-txt-generator.js prod",
    "robots-txt:generate:dev": "node scripts/run.robots-txt-generator.js dev",
    "routes:generate:prod": "node scripts/run.routes-generator.js prod",
    "routes:generate:dev": "node scripts/run.routes-generator.js dev",
    "htaccess:generate": "node scripts/run.prepare.htaccess.js",
    "404-prerender:prepare": "node scripts/run.404-prerender.prepare.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/platform-server": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@nguniversal/express-engine": "^15.2.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@sentry/angular-ivy": "^7.81.1",
    "@sentry/node": "^7.81.1",
    "countries-list": "^3.0.1",
    "date-fns": "^2.30.0",
    "express": "^4.15.2",
    "kursor": "^0.1.6",
    "localstorage-polyfill": "^1.0.1",
    "ng-lazyload-image": "^9.1.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.1",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^15.0.0",
    "@angular-devkit/build-angular": "^15.2.9",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.9",
    "@angular/compiler-cli": "^15.2.9",
    "@nguniversal/builders": "^15.2.1",
    "@sentry/types": "^7.81.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "^8.33.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "^3.0.3",
    "typescript": "~4.9.4"
  }
}
